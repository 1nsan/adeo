      function testSearchValue(type) {
          if (type == 1) {
              if ($j("#chps_search").val() == "") {
                  $j("#chps_search").val("");
              } else if (/\S$/.test($j("#chps_search").val())) {
                  $j("#chps_search").val($j("#chps_search").val() + " ");
              }
          } else {
              if ($j("#chps_search").val() == '') {
                  $j("#chps_search").val("")
              }
          }
      }

      function testCookiePolicy() {
          if(getCookie("CookiePolicy")=="") {
              $j('#cookiePoolicy').css('display', 'inline');
          }
      }
      function closeLayerCookiePolicy() {
          if(getCookie("CookiePolicy")=="") {
              document.cookie = "CookiePolicy=true; max-age=" + 60 * 60 * 24 * 395;
              $j('#cookiePoolicy').hide();
          }
      }
